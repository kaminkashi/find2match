    /* Styles for the transparent popup */
    .popup-overlay {
      display: none;
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.5);
      justify-content: center;
      align-items: center;
      z-index: 999;
    }

    .popup-content {
      position: relative;
      background: linear-gradient(to right, #3498db, #8e44ad);
      color: #fff;
      padding: 20px;
      border-radius: 10px;
      box-shadow: 0 0 20px rgba(0, 0, 0, 0.5);
      text-align: center;
      max-width: 300px;
      margin: auto;
    }

    .cut-option {
      position: absolute;
      top: 5px;
      right: 5px;
      cursor: pointer;
      color: #fff;
      font-size: 20px;
    }

    /* Add radius to the header */
    header {
      background: linear-gradient(to right, #3498db, #8e44ad);
      position: relative;
      height: 40px;
      padding: 10px;
      border-bottom-left-radius: 15px;
      border-bottom-right-radius: 15px;
      bottom: 22vh;
    }

    .login-option {
      text-align: right;
      margin-right: 20px;
    }

    .loginBtn {
      color: #fff;
      text-decoration: none;
      padding: 5px 10px;
      border: 1px solid #fff;
      border-radius: 5px;
    }

    .loginBtn:hover {
      background: #fff;
      color: #3498db;
    }

    .loading-container {
      text-align: center;
    }


    @keyframes spin {
      0% {
        transform: rotate(0deg);
      }

      100% {
        transform: rotate(360deg);
      }
    }

    /* CSS for the card */
    .card {
      border: 2px solid #ccc;
      border-radius: 8px;
      padding: 20px;
      margin: 20px;
      max-width: 300px;
    }

    /* CSS for the legend */
    legend {
      font-weight: bold;
      color: #333;
      font-size: 1.2em;
      margin-bottom: 10px;
    }

    /* Styling the form elements */
    input[type="text"],
    button {
      padding: 8px;
      margin-bottom: 10px;
      width: 100%;
      box-sizing: border-box;
    }

    button {

  <script>
    const portfolioTitles = document.querySelectorAll('.portfolio-title');

    portfolioTitles.forEach(title => {
      title.addEventListener('mouseover', () => {
        title.classList.add('shine-animation');
      });

      title.addEventListener('mouseout', () => {
        title.classList.remove('shine-animation');
      });
    });

    // Array of different contents
    const contents = [
      "This is my portfolio",
      "Welcome to my website",
      "Explore my projects here",
      "Get to know my work",
      // Add more content as needed
    ];

    // Function to display content with typing animation
    function displayContent() {
      const heading = document.getElementById('portfolio-heading');
      let index = 0;
      setInterval(() => {
        heading.innerHTML = `<span class="typing-animation">${contents[index]}</span>`;
        index = (index + 1) % contents.length;
      }, 5000); // Change content every 5 seconds (5000 milliseconds)
    }

    // Call the function to start displaying content
    displayContent();

    // Array of image URLs
    const imageUrls = [
      "images/AmarKumar.png",
      "images/Recording 2024-03-05 234435.gif",
      "images/Screenshot 2024-03-08 215101.png",
      "images/Untitled design (4).png",
      // Add more image URLs as needed
    ];

    // Function to change image at regular intervals
    function changeImage() {
      const bannerImg = document.querySelector('.banner-img');
      let index = 0;
      setInterval(() => {
        bannerImg.style.opacity = 0; // Fade out
        setTimeout(() => {
          bannerImg.src = imageUrls[index];
          index = (index + 1) % imageUrls.length;
          bannerImg.style.opacity = 1; // Fade in
        }, 500); // Wait for 500 milliseconds before changing the image
      }, 5000); // Change image every 5 seconds (5000 milliseconds)
    }

    // Call the function to start changing images
    changeImage();


  </script>
